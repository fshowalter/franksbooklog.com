---
import { getProps } from "~/components/Shelf/getProps";
import { Shelf } from "~/components/Shelf/Shelf";
import Layout from "~/layouts/Layout.astro";

const props = await getProps();
---

<Layout
  article={false}
  canonical={true}
  description={props.deck}
  title="The Shelf"
>
  <Shelf client:load {...props} />
</Layout>
